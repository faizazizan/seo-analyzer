{
    "version": 2,
    {
        "version": 2,
        "builds": [
            {
                "src": "server.js",
                "use": "@vercel/node"
            }
        ],
        "routes": [
            {
                "src": "/api/(.*)",
                "dest": "/server.js"
            },
            {
                "src": "/style.css",
                "dest": "/style.css"
            },
            {
                "src": "/script.js",
                "dest": "/script.js"
            },
            {
                "src": "/",
                "dest": "/index.html"
            },
            {
                "src": "/(.*)",
                "dest": "/$1"
            }
        ]
    }